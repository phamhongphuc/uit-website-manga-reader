{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 120,
    "overrides": [
        {
            "files": ["*.code-workspace", ".stylelintrc", ".babelrc"],
            "options": { "parser": "json" }
        }
    ]
}
